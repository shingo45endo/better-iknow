{
	"manifest_version": 2,
	"name": "Better iKnow!",
	"version": "0.1",

	"description": "Makes iKnow dictation app easier to type. Adds some improvements.",

	"content_scripts": [{
		"matches": ["http://iknow.jp/apps/*"],
		"js": [
			"content_script.js"
		],
		"run_at": "document_end"
	}],
	"web_accessible_resources": [
		"embeded_script.js"
	]
}
